   "source": [
    "def mukemmelBul(sayi):\n",
    "    toplam=0\n",
    "    for i in range(1,sayi):\n",
    "        if(sayi%i==0):\n",
    "            toplam+=i\n",
    "    if(toplam==sayi):\n",
    "        return True\n",
    "    else:\n",
    "        return False\n",
    "\n",
    "for i in range(1,1001):\n",
    "    if(mukemmelBul(i)):\n",
    "        print(i,\" mükemmel sayıdır.\")\n"
   ]
