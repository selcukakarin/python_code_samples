     "text": [
      "sayı gir : 6\n",
      "bu sayı armstrong\n"
     ]
    }
   ],
   "source": [
    "sayi=input(\"sayı gir : \")\n",
    "toplam=0\n",
    "basamak=len(sayi)\n",
    "sayi=int(sayi)\n",
    "tempsayi=sayi\n",
    "\n",
    "while(tempsayi>0):\n",
    "    toplam+=((tempsayi%10)**basamak)\n",
    "    tempsayi//=10\n",
    "if(toplam==sayi):\n",
    "    print(\"bu sayı armstrong\")\n",
    "else:\n",
    "    print(\"bu sayı armstrong değil\")"
   ]
